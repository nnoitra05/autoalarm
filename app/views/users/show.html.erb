<div class="mypage">
    <div class="mypage_title">
      <h1>マイページ</h1>
    </div>
    <div class="user_name">
      <%= current_user.nickname %><br>
      <%= current_user.email %><br>
    </div>
    <div class="user_btn">
      <%= link_to "編集する", edit_user_path(current_user.id), class: 'btn'%>
      <%= link_to "削除する", user_path(current_user.id), method: :delete, class: 'btn'%>
    </div>
</div>